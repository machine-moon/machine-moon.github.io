{
  "name": "frontend",
  "version": "1.0.0",
  "private": true,
  "description": "A frontend project for showcasing a portfolio website.",
  "type": "module",
  "homepage": "https://machine-moon.github.io/machine-moon.github.io",
  "scripts": {
    "dev": "vite --config ./config/vite.config.js",
    "predeploy": "yarn build",
    "deploy": "gh-pages -d dist",
    "build": "vite build --config ./config/vite.config.js",
    "lint": "eslint . --config ./config/.eslintrc.cjs --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "format": "prettier --write .",
    "test": "jest --config ./config/jest.config.js",
    "preview": "vite preview --config ./config/vite.config.js"
  },
  "dependencies": {
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@mui/icons-material": "^5.15.20",
    "@mui/material": "^5.15.20",
    "react": "^18.2.0",
    "react-anchor-link-smooth-scroll": "^1.0.12",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.23.1",
    "typewriter-effect": "^2.21.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-react": "^7.26.3",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@vitejs/plugin-react": "^4.2.1",
    "babel-jest": "^29.7.0",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "gh-pages": "^6.2.0",
    "jest": "^29.7.0",
    "prettier": "^2.8.0",
    "vite": "^5.2.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/machine-moon/machine-moon.github.io.git"
  },
  "bugs": {
    "url": "https://github.com/machine-moon/machine-moon.github.io/issues"
  },
  "keywords": [
    "portfolio",
    "react",
    "vite",
    "mui",
    "web3forms"
  ],
  "author": "Tarek Ibrahim <tarekibrahim3@cmail.carleton.ca>",
  "license": "MIT"
}
